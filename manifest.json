{
    "manifest_version": 2,
    "name": "Milli-Zeit",
    "description": "Converts timestamps and durations in milliseconds to human readable format.",
    "version": "0.9",
    "content_scripts": [{
            "all_frames": true,
            "js": ["content_script.js"],
            "matches": ["http://*/*", "https://*/*"],
            "run_at": "document_start"
        }],
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}